{
	"name": "@pixelated-tech/components",
	"version": "3.4.3",
	"private": false,
	"author": {
		"name": "Pixelated Technologies",
		"url": "https://www.pixelated.tech/"
	},
	"description": "Pixelated UI Components",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/types/index.d.ts",
	"files": [
		"dist"
	],
	"directories": {
		"doc": "docs"
	},
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./server": {
			"types": "./dist/types/index.server.d.ts",
			"import": "./dist/index.server.js",
			"default": "./dist/index.server.js"
		},
		"./dist/components/*": "./dist/components/*",
		"./css/*": "./dist/css/*"
	},
	"homepage": "https://github.com/brianwhaley/pixelated-components#readme",
	"bugs": {
		"url": "https://github.com/brianwhaley/pixelated-components/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brianwhaley/pixelated-components.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"browser": {
		"fs": false,
		"path": false
	},
	"scripts": {
		"build": "npm run validate-exports && npm run buildClean && npm run tsc && npm run rsync && npm run tscClean ",
		"build-webpack": "webpack --config webpack.config.cjs && tsc --emitDeclarationOnly --outDir dist/types",
		"buildClean": "rm -r dist ",
		"tsc": "npx tsc --project tsconfig.json",
		"tscClean": "rm -r dist/{images,stories,tests} ",
		"rsync": "rsync -a --include='*.css' --include='*.scss' --include='*.json' --include='*/' --exclude='*' src/ dist",
		"validate-exports": "node scripts/validate-exports.cjs",
		"copy2brianwhaley": "rm -rf ../brianwhaley/node_modules/@pixelated-tech/components/dist && cp -r dist ../brianwhaley/node_modules/@pixelated-tech/components/",
		"copy2informationfocus": "rm -rf ../informationfocus/node_modules/@pixelated-tech/components/dist && cp -r dist ../informationfocus/node_modules/@pixelated-tech/components/",
		"copy2oaktreelandscaping": "rm -rf ../oaktreelandscaping/node_modules/@pixelated-tech/components/dist && cp -r dist ../oaktreelandscaping/node_modules/@pixelated-tech/components/",
		"copy2palmettoepoxy": "rm -rf ../palmetto-epoxy/node_modules/@pixelated-tech/components/dist && cp -r dist ../palmetto-epoxy/node_modules/@pixelated-tech/components/",
		"copy2pixelated": "rm -rf ../pixelated/node_modules/@pixelated-tech/components/dist && cp -r dist ../pixelated/node_modules/@pixelated-tech/components/",
		"copy2pixelvivid": "rm -rf ../pixelvivid/node_modules/@pixelated-tech/components/dist && cp -r dist ../pixelvivid/node_modules/@pixelated-tech/components/",
		"copy2pixelatedtest": "rm -rf ../pixelated-test/node_modules/@pixelated-tech/components/dist && cp -r dist ../pixelated-test/node_modules/@pixelated-tech/components/",
		"copy2template": "rm -rf ../pixelated-template/node_modules/@pixelated-tech/components/dist && cp -r dist ../pixelated-template/node_modules/@pixelated-tech/components/",
		"copy2pixelatedadmin": "rm -rf ../pixelated-admin/node_modules/@pixelated-tech/components/dist && cp -r dist ../pixelated-admin/node_modules/@pixelated-tech/components/",
		"copy2all": "npm run copy2brianwhaley && npm run copy2informationfocus && npm run copy2oaktreelandscaping && npm run copy2palmettoepoxy && npm run copy2pixelated && npm run copy2pixelvivid && npm run copy2pixelatedtest && npm run copy2template && npm run copy2pixelatedadmin",
		"storybook": "rm -rf node_modules/.cache && storybook dev -p 6006",
		"buildStorybook": "rm -rf node_modules/.cache && NODE_OPTIONS=\"--max-old-space-size=4096\" storybook build",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:run": "vitest run",
		"lint": "eslint --fix"
	},
	"scripts-old": {
		"buildBabel": "npm run buildClean && NODE_ENV=production babel src --out-dir dist --copy-files",
		"build-webpack": "rm -rf dist && npx tsc --project tsconfig.json && webpack --config webpack.config.js && npm run build-webpack-rsync",
		"build-webpack-rsync": "rsync -a --include='*' --include='*/' src/css dist/css"
	},
	"dependencies": {
		"@aws-sdk/client-cloudwatch": "^3.958.0",
		"@aws-sdk/client-route-53": "^3.958.0",
		"date-fns": "^4.1.0",
		"globals": "^17.0.0",
		"googleapis": "^169.0.0",
		"html-entities": "^2.6.0",
		"puppeteer": "^24.34.0",
		"recharts": "^3.6.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.28.3",
		"@babel/core": "^7.28.5",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/preset-env": "^7.28.5",
		"@babel/preset-react": "^7.28.5",
		"@babel/preset-typescript": "^7.28.5",
		"@eslint/json": "^0.14.0",
		"@eslint/markdown": "^7.5.1",
		"@storybook/addon-webpack5-compiler-babel": "^4.0.0",
		"@storybook/preset-scss": "^1.0.3",
		"@storybook/react-webpack5": "^10.1.11",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/md5": "^2.3.6",
		"@types/node": "^25.0.3",
		"@types/prop-types": "^15.7.15",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.51.0",
		"@typescript-eslint/parser": "^8.51.0",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/ui": "^4.0.16",
		"ajv": "^8.17.1",
		"ajv-keywords": "^5.1.0",
		"babel-loader": "^10.0.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^13.0.1",
		"css-loader": "^7.1.2",
		"eslint": "^9.39.2",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-n": "^17.23.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-storybook": "^10.1.11",
		"file-loader": "^6.2.0",
		"happy-dom": "^20.0.11",
		"jsdom": "^27.4.0",
		"less-loader": "^12.3.0",
		"mini-css-extract-plugin": "^2.9.4",
		"next": "^16.1.1",
		"null-loader": "^4.0.1",
		"prop-types": "^15.8.1",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-test-renderer": "^19.2.3",
		"sass": "^1.97.1",
		"sass-loader": "^16.0.6",
		"storybook": "^10.1.11",
		"style-loader": "^4.0.0",
		"ts-loader": "^9.5.4",
		"typescript": "^5.9.3",
		"url-loader": "^4.1.1",
		"vitest": "^4.0.16",
		"webpack": "^5.104.1",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^5.2.2",
		"webpack-node-externals": "^3.0.0"
	},
	"peerDependencies": {
		"next": "^16.0.3",
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"optionalDependencies": {
		"md5": "^2.3.0"
	},
	"type": "module"
}

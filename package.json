{
  "name": "pixelatedtech",
  "version": "4.4.6",
  "private": true,
  "author": {
    "name": "Pixelated Technologies",
    "url": "https://www.pixelated.tech/"
  },
  "scripts": {
    "dev": "npm run clean && NODE_OPTIONS='--inspect' next dev  -p 3004 --experimental-https",
    "build": "npm run clean && NODE_ENV=production next build",
    "start": "npm run clean && next build && next start",
    "lint": "eslint --fix",
    "clean": "rm -rf .next && rm -rf out",
    "generate-site-images": "node ./node_modules/@pixelated-tech/components/dist/scripts/generate-site-images.js",
    "config:vault": "npx tsx ./node_modules/@pixelated-tech/components/dist/scripts/config-vault.ts",
    "config:encrypt": "npm run config:vault -- encrypt src/app/config/pixelated.config.json",
    "config:decrypt": "npm run config:vault -- decrypt src/app/config/pixelated.config.json",
    "release": "./node_modules/@pixelated-tech/components/dist/scripts/release.sh"
  },
  "scripts-unused": {
    "dev": "next dev --turbopack"
  },
  "dependencies": {
    "@pixelated-tech/components": "^3.7.9",
    "date-fns": "^4.1.0",
    "globals": "^17.0.0",
    "html-entities": "^2.6.0",
    "md5": "^2.3.0",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@next/eslint-plugin-next": "^16.1.1",
    "@next/third-parties": "^16.1.1",
    "@types/node": "^25.0.3",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-react": "^7.37.5",
    "sass": "^1.97.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
